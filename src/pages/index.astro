	content="Empresa que trabaja en el sector del diseño gráfico y la programación de páginas web para dar vida a tu marca">
<meta property="og:image" content="images/portada1.png">
<meta property="og:url" content="https://saraquintana.es">
<meta property="og:type" content="website">
<meta property="og:locale" content="es_ES">
<meta property="og:site_name" content="Sara Quintana Diseño gráfico y branding">
<meta property="og:image:alt" content="Estás a un click de cambiar tu futuro">
<meta name="viewport" content="width=device-width, initial-scale=1.0">

<Layout
	title="SaraQuintana - Desarrollo de imagen de marca, dieño web y marketing"
	description="Descubre como renobar tu imagen de marca, diseño web y marketing con SaraQuintana"
>
	<Header />
	<Video />

	<main>
		<Intro />
		<Bento />
		<PreFooter />
	</main>
	<Contacto />
	<Empresas />
	<Footer />
